package com.potmo.util.color{
	public class XYZColor{
		
		/** X from 0.0  **/
		public var x:Number;
		
		/** Y from 0.0 **/
		public var y:Number;
		
		/** Z from 0.0 **/
		public var z:Number;
		
		public function XYZColor(x:Number, y:Number, z:Number)
		{
			this.x = x;
			this.y = y;
			this.z = z;
		}
	}
}