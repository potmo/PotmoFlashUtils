package com.potmo.util.color{
	public class YXYColor{
		/** Y from 0.0  **/
		public var Y:Number;
		
		/** x from 0.0 **/
		public var x:Number;
		
		/** y from 0.0 **/
		public var y:Number;
		
		public function YXYColor(Y:Number, x:Number, y:Number)
		{
			this.Y = y;
			this.x = x;
			this.y = y;
		}
	}
}